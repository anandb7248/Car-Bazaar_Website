        </from-view-id>
        <navigation-case>
            <from-outcome>setting</from-outcome>
            <to-view-id>/setting.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>                                                                                                                                                     
    
    <!--Navigation for buying page-->
